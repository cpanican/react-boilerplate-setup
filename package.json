{
  "name": "react-boilerplate-setup",
  "version": "1.0.0",
  "description": "Minimal React, Webpack, Babel, Jest and Eslint setup",
  "main": "index.js",
  "scripts": {
    "build": "./node_modules/.bin/webpack --mode production",
    "lint": "./node_modules/.bin/eslint --ext .jsx --ext .js --ignore-path .gitignore .",
    "lint:fix": "./node_modules/.bin/eslint --fix --ignore-path .gitignore .",
    "start": "./node_modules/.bin/webpack-dev-server --mode development --open",
    "test": "./node_modules/.bin/jest --passWithNoTests",
    "test:coverage": "./node_modules/.bin/jest --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cpanican/react-boilerplate-setup.git"
  },
  "keywords": [
    "React"
  ],
  "author": "Chris Panican",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cpanican/react-boilerplate-setup/issues"
  },
  "homepage": "https://github.com/cpanican/react-boilerplate-setup#readme",
  "dependencies": {
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  },
  "devDependencies": {
    "@babel/core": "^7.4.3",
    "@babel/preset-env": "^7.4.3",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.5",
    "css-loader": "^2.1.1",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jest": "^22.4.1",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.12.4",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^24.7.1",
    "style-loader": "^0.23.1",
    "webpack": "^4.30.0",
    "webpack-cli": "^3.3.0",
    "webpack-dev-server": "^3.3.1"
  }
}
