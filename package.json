{
  "name": "react-boilerplate-setup",
  "version": "1.0.0",
  "description": "Minimal React, Webpack, Babel, Jest and Eslint setup",
  "main": "index.js",
  "scripts": {
    "build": "./node_modules/.bin/webpack --mode production",
    "lint": "./node_modules/.bin/eslint --ext .jsx --ext .js --ignore-path .gitignore .",
    "lint:fix": "./node_modules/.bin/eslint --fix --ignore-path .gitignore .",
    "start": "./node_modules/.bin/webpack-dev-server --mode development --open",
    "test": "./node_modules/.bin/jest --passWithNoTests",
    "test:coverage": "./node_modules/.bin/jest --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cpanican/react-boilerplate-setup.git"
  },
  "keywords": [
    "React"
  ],
  "author": "Chris Panican",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cpanican/react-boilerplate-setup/issues"
  },
  "homepage": "https://github.com/cpanican/react-boilerplate-setup#readme",
  "dependencies": {
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  },
  "devDependencies": {
    "@babel/core": "^7.5.4",
    "@babel/preset-env": "^7.5.4",
    "@babel/preset-react": "^7.0.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^23.4.2",
    "babel-loader": "^8.0.6",
    "css-loader": "^2.1.1",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.1",
    "eslint-loader": "^2.2.1",
    "eslint-plugin-import": "^2.18.0",
    "eslint-plugin-jest": "^22.7.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.14.2",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^24.8.0",
    "style-loader": "^0.23.1",
    "webpack": "^4.35.3",
    "webpack-cli": "^3.3.6",
    "webpack-dev-server": "^3.7.2"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/__mocks__/styleMock.js"
    }
  }
}
